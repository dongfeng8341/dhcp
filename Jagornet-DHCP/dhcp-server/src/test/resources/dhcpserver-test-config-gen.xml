<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:dhcpServerConfig xmlns:ns2="http://jagornet.com/dhcp/xml">
    <v6ServerIdOption>
        <opaqueData>
            <hexValue>000100015DDB4DADF239C8338A18</hexValue>
        </opaqueData>
    </v6ServerIdOption>
    <v4ServerIdOption>
        <ipAddress>10.10.10.216</ipAddress>
    </v4ServerIdOption>
    <policies>
        <policy>
            <name>dhcp.ignoreSelfPackets</name>
            <value>false</value>
        </policy>
    </policies>
    <links>
        <link>
            <name>Test IPv4 Client Link</name>
            <address>10.10.10.0/24</address>
            <v4ConfigOptions>
                <v4SubnetMaskOption>
                    <ipAddress>255.255.255.0</ipAddress>
                </v4SubnetMaskOption>
                <v4RoutersOption>
                    <ipAddress>10.10.10.1</ipAddress>
                </v4RoutersOption>
                <v4DomainServersOption>
                    <ipAddress>10.0.0.1</ipAddress>
                </v4DomainServersOption>
                <v4DomainNameOption>
                    <string>test.jagornet.com.</string>
                </v4DomainNameOption>
            </v4ConfigOptions>
            <v4AddrPools>
                <pool>
                    <range>10.10.10.200-10.10.10.250</range>
                </pool>
            </v4AddrPools>
        </link>
        <link>
            <name>Test IPv6 Client Link</name>
            <interface>en0</interface>
            <v6MsgConfigOptions>
                <v6DnsServersOption>
                    <ipAddress>2001:db8:1::1</ipAddress>
                </v6DnsServersOption>
                <v6DomainSearchListOption>
                    <domainName>test.jagornet.com.</domainName>
                </v6DomainSearchListOption>
            </v6MsgConfigOptions>
            <v6NaAddrPools>
                <pool>
                    <range>2001:db8:1::/64</range>
                </pool>
            </v6NaAddrPools>
        </link>
    </links>
</ns2:dhcpServerConfig>
